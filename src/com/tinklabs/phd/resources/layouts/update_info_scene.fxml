<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<BorderPane id="BorderPane" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2">
    <top>
        <VBox alignment="CENTER">
            <children>
                <Text text="You have the latest version">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                    <font>
                        <Font style="bold" size="16.0" />
                    </font>
                </Text>
            </children>
        </VBox>
    </top>
    <center>
        <VBox fx:id="vbox" alignment="CENTER">
            <children>
                <VBox fx:id="dynamic_info_container" alignment="CENTER"></VBox>
                <Button fx:id="next" alignment="center" text="Next">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>

            </children>
        </VBox>
    </center>

</BorderPane>
